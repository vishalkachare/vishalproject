{
	"name": "df_wafa_unpivot",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_Wafa_deli_TxtFile",
						"type": "DatasetReference"
					},
					"name": "PurchaseOrder"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "unpivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PO as integer,",
				"          Vendor as string,",
				"          Apple as string,",
				"          Mango as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PurchaseOrder",
				"PurchaseOrder unpivot(output(",
				"          Fruits as string,",
				"          Amount as string",
				"     ),",
				"     ungroupBy(PO,",
				"          Vendor),",
				"     lateral: true,",
				"     ignoreNullPivots: false) ~> unpivot1"
			]
		}
	}
}